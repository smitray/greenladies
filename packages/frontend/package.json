{
	"name": "@greenladies/frontend",
	"version": "0.0.1",
	"repository": "greenladies/frontend",
	"private": true,
	"scripts": {
		"build": "yarn build:check && webpack --config webpack.prod.ts",
		"build:check": "yarn --cwd ../backend generate-graphql-schema && yarn relay && tsc --noEmit",
		"clean": "rimraf dist **/__generated__",
		"dev": "concurrently \"yarn relay:watch\" \"babel-node --extensions \".ts,.tsx\" index.ts\"",
		"lint": "eslint --ext .ts,.tsx --ignore-path ../../.gitignore . && yarn prettier:check",
		"lint:fix": "eslint --ext .ts,.tsx --ignore-path ../../.gitignore --fix . && yarn prettier:format",
		"prettier:check": "prettier --list-different --ignore-path ../../.gitignore \"**/*.{js,jsx,json,yml,yaml}\"",
		"prettier:format": "prettier --write --ignore-path ../../.gitignore --loglevel error \"**/*.{js,jsx,json,yml,yaml}\"",
		"relay": "relay-compiler",
		"relay:watch": "relay-compiler --watch",
		"test": "jest --passWithNoTests",
		"webpack:watch": "webpack-dev-server --config webpack.dev.ts"
	},
	"dependencies": {
		"graphql": "^15.3.0",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-relay": "^10.0.0",
		"react-relay-network-modern": "^4.7.3",
		"react-relay-network-modern-ssr": "^1.4.0"
	},
	"devDependencies": {
		"@artsy/express-reloadable": "^1.4.8",
		"@babel/core": "^7.10.5",
		"@babel/node": "^7.10.5",
		"@babel/plugin-proposal-optional-chaining": "^7.10.4",
		"@babel/preset-env": "^7.10.4",
		"@babel/preset-react": "^7.10.4",
		"@babel/preset-typescript": "^7.10.4",
		"@types/dotenv-webpack": "^1.8.0",
		"@types/morgan": "^1.9.1",
		"@types/react": "^16.9.43",
		"@types/react-dom": "^16.9.8",
		"@types/react-relay": "^7.0.8",
		"@types/webpack": "^4.41.21",
		"@types/webpack-dev-middleware": "^3.7.1",
		"@types/webpack-hot-middleware": "^2.25.3",
		"babel-loader": "^8.1.0",
		"babel-plugin-relay": "^10.0.0",
		"dotenv-webpack": "^2.0.0",
		"fork-ts-checker-webpack-plugin": "^5.0.7",
		"morgan": "^1.10.0",
		"relay-compiler-language-typescript": "^12.0.3",
		"relay-config": "^10.0.0",
		"ts-node": "^8.10.2",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.12",
		"webpack-dev-middleware": "^3.7.2",
		"webpack-hot-middleware": "^2.25.0",
		"webpack-merge": "^5.0.9"
	}
}
