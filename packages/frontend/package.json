{
	"name": "@greenladies/frontend",
	"version": "0.0.1",
	"repository": "greenladies/frontend",
	"private": true,
	"scripts": {
		"build": "yarn --cwd ../backend generate-graphql-schema && yarn generate-graphql-types && next build",
		"clean": "rimraf dist **/__generated__",
		"dev": "concurrently \"yarn generate-graphql-types:watch\" \"next\"",
		"generate-graphql-types": "apollo client:codegen --target typescript",
		"generate-graphql-types:watch": "yarn generate-graphql-types --watch",
		"lint": "eslint --ext .ts,.tsx --ignore-path ../../.gitignore . && yarn prettier:check",
		"lint:fix": "eslint --ext .ts,.tsx --ignore-path ../../.gitignore --fix . && yarn prettier:format",
		"prettier:check": "prettier --list-different --ignore-path ../../.gitignore \"**/*.{js,jsx,json,yml,yaml}\"",
		"prettier:format": "prettier --write --ignore-path ../../.gitignore --loglevel error \"**/*.{js,jsx,json,yml,yaml}\"",
		"start": "next start",
		"test": "jest --passWithNoTests"
	},
	"dependencies": {
		"@apollo/client": "^3.0.1",
		"next": "^9.4.4",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"styled-components": "^5.1.1"
	},
	"devDependencies": {
		"@types/react": "^16.9.43",
		"@types/styled-components": "^5.1.1",
		"apollo": "^2.30.1",
		"babel-plugin-styled-components": "^1.10.7",
		"eslint-plugin-react": "^7.20.3",
		"eslint-plugin-react-hooks": "^4.0.8",
		"ts-node": "^8.10.2"
	}
}
